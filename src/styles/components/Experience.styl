.Experience-container
  background #bdf1d4
  padding 10px
  border-radius 10px
  margin 15px 0

.Experience-list
  list-style none
  margin 0
  padding 0
  display grid
  grid-template-columns 1fr 1fr 

.Experience-item
  border 1px solid grey
  margin 5px 0
  border-radius 5px
  padding 10px
  width fit-content
  margin 0 2px

.Item-link
  color red
  font-style bold

.Experiente-item_title
  font-style italic
  font-size 1.2rem